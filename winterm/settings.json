{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        { "command": "find", "keys": "ctrl+shift+f" },
        { "command": "selectAll", "keys": "ctrl+a" },
        {
            "command": { "action": "copy", "singleLine": false },
            "keys": "ctrl+shift+c"
        },

        { "command": { "action": "scrollUp", "rowsToScroll": 5 }, "keys": "ctrl+up" },
        { "command": { "action": "scrollDown", "rowsToScroll": 5 }, "keys": "ctrl+down" },
        { "command": "scrollUpPage", "keys": "ctrl+pgup" },
        { "command": "scrollDownPage", "keys": "ctrl+pgdn" },
        
        {
            "command": { "action": "splitPane", "split": "auto", "splitMode": "duplicate" },
            "keys": "alt+d"
        },
        {
            "command": { "action": "splitPane", "split": "down" },
            "keys": "alt+v"
        },
        {
            "command": { "action": "splitPane", "split": "right" },
            "keys": "alt+c"
        },
        { "command": "closePane", "keys": "alt+w" },
        {
            "command": { "action": "moveFocus", "direction": "previous" },
            "keys": "alt+s"
        },
        
        { "command": "newTab", "keys": "ctrl+shift+t" },
        { "command": "duplicateTab", "keys": "ctrl+shift+d" },
        { "command": "closeTab", "keys": "ctrl+shift+w" },
        
        { "command": { "action": "switchToTab", "index": 0 }, "keys": "ctrl+1" },
        { "command": { "action": "switchToTab", "index": 1 }, "keys": "ctrl+2" },
        { "command": { "action": "switchToTab", "index": 2 }, "keys": "ctrl+3" },
        { "command": { "action": "switchToTab", "index": 3 }, "keys": "ctrl+4" },
        { "command": { "action": "switchToTab", "index": 4 }, "keys": "ctrl+5" },
        { "command": { "action": "switchToTab", "index": 5 }, "keys": "ctrl+6" },
        { "command": { "action": "switchToTab", "index": 6 }, "keys": "ctrl+7" },
        { "command": { "action": "switchToTab", "index": 7 }, "keys": "ctrl+8" },
        { "command": { "action": "switchToTab", "index": 8 }, "keys": "ctrl+9" },
        
        { "command": "unbound", "keys": "enter" },
        { "command": "unbound", "keys": "ctrl+insert" },
        { "command": "unbound", "keys": "alt+shift+d" },
        { "command": "unbound", "keys": "ctrl+alt+left" },
        { "command": "unbound", "keys": "shift+insert" },
        { "command": "unbound", "keys": "ctrl+shift+a" },
        { "command": "unbound", "keys": "alt+shift+minus" },
        { "command": "unbound", "keys": "alt+shift+plus" },
        { "command": "unbound", "keys": "ctrl+alt+1" },
        { "command": "unbound", "keys": "ctrl+alt+2" },
        { "command": "unbound", "keys": "ctrl+alt+3" },
        { "command": "unbound", "keys": "ctrl+alt+4" },
        { "command": "unbound", "keys": "ctrl+alt+5" },
        { "command": "unbound", "keys": "ctrl+alt+6" },
        { "command": "unbound", "keys": "ctrl+alt+7" },
        { "command": "unbound", "keys": "ctrl+alt+8" },
        { "command": "unbound", "keys": "ctrl+alt+9" },
        { "command": "unbound", "keys": "ctrl+w" },
        { "command": "unbound", "keys": "ctrl+d" },
        { "command": "unbound", "keys": "ctrl+shift+up" },
        { "command": "unbound", "keys": "ctrl+shift+down" },
        { "command": "unbound", "keys": "ctrl+shift+pgup" },
        { "command": "unbound", "keys": "ctrl+shift+pgdn" },
    ],
    "centerOnLaunch": true,
    "confirmCloseAllTabs": false,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{7e0ae011-4ba8-4095-9efb-0a265ba2b480}",
    "firstWindowPreference": "persistedWindowLayout",
    "initialCols": 150,
    "initialRows": 50,
    "launchMode": "focus",
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": 
    {
        "defaults": 
        {
            "colorScheme": "Catppuccin Mocha",
            "font": 
